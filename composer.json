{
    "name": "fibraclick/flarum-tweaks",
    "description": "FibraClick",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0.0",
        "guzzlehttp/guzzle": "^7.2",
        "flarum/flags": "^1.0.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "FibraClick\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "FibraClick",
            "icon": {
                "image": "icon.png",
                "backgroundColor": "#3fc160",
                "color": "#ffffff"
            },
            "optional-dependencies": [
                "flarum/tags"
            ]
        },
        "flagrow": {
            "discuss": ""
        }
    }
}
